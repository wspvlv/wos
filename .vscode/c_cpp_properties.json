{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/prj/wlib/inc",
                "./inc"
            ],
            "defines": [
                "OS_WOS"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-ffreestanding", 
                "-Ofast",
                "-Wall",
                "-Wextra",
                "-Wpedantic",
                "-Werror",
                "-Iinc", 
                "-nostdlib",
                "std=c2x"
            ]
        }
    ],
    "version": 4
}